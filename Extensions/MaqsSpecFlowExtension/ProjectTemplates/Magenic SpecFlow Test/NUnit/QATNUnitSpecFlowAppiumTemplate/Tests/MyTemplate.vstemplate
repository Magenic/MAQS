<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>MAQS SpecFlow Appium Tests</Name>
    <Description>Magenic's automation quick start framework for Appium testing using SpecFlow</Description>
    <ProjectType>CSharp</ProjectType>
    <LanguageTag>C#</LanguageTag>
    <PlatformTag>windows</PlatformTag>
    <ProjectTypeTag>Magenic Test</ProjectTypeTag>
    <ProjectSubType>Magenic Test</ProjectSubType>
    <DefaultName>AppiumTests</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <CreateInPlace>true</CreateInPlace>
    <Icon>__TemplateIcon.ico</Icon>
    <Hidden>true</Hidden> 
    <CreateInPlace>true</CreateInPlace>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="Tests.csproj" File="Tests.csproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true" TargetFileName="App.config">App.config</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="AppiumFeature.feature">AppiumFeature.feature</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="AppiumFeatureSteps.cs">AppiumFeatureSteps.cs</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>