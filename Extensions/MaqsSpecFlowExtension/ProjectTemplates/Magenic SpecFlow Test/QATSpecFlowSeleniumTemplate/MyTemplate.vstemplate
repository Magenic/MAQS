<VSTemplate Version="3.0.0" Type="ProjectGroup"
    xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
    <TemplateData>
    <Name>MAQS SpecFlow Selenium Tests</Name>
    <Description>Magenic's automation quick start framework for Selenium testing using SpecFlow</Description>
    <DefaultName>SeleniumTests</DefaultName>
    <Icon>QATTest.ico</Icon>
    <ProjectType>CSharp</ProjectType>
    <LanguageTag>C#</LanguageTag>
    <PlatformTag>windows</PlatformTag>
    <ProjectTypeTag>test</ProjectTypeTag>
    <ProjectTypeTag>Magenic Test</ProjectTypeTag>
    <ProjectSubType>Magenic Test</ProjectSubType>
    </TemplateData>
    <TemplateContent>
        <ProjectCollection>
                <ProjectTemplateLink ProjectName="PageModel">
                    PageModel\MyTemplate.vstemplate
                </ProjectTemplateLink>
                <ProjectTemplateLink ProjectName="Tests">
                    Tests\MyTemplate.vstemplate
                </ProjectTemplateLink>
        </ProjectCollection>
    </TemplateContent>
</VSTemplate>